{
  "name": "nba-stats-client",
  "version": "0.0.0-development",
  "description": "NodeJS Client for accessing NBA.com statistics",
  "files": [
    "build"
  ],
  "browser": "build/index.js",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "global": false,
  "scripts": {
    "build": "BABEL_ENV=production rollup -c",
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "deploy": "npm run travis-deploy-once 'npm run semantic-release'",
    "gc": "commit",
    "is-es5": "es-check es5 build/index.js",
    "lint": "eslint --ext .js .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "test": "BABEL_ENV=test jest src/ --coverage --no-cache",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaebradley/nba-stats-client.git"
  },
  "keywords": [
    "nba",
    "nba statistics",
    "nba client",
    "nba api"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/nba-stats-client/issues"
  },
  "homepage": "https://github.com/jaebradley/nba-stats-client#readme",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0-beta.51",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@commitlint/prompt": "^8.1.0",
    "@commitlint/prompt-cli": "^8.1.0",
    "ajv": "^6.5.1",
    "axios-debug": "0.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.2.0",
    "babel-preset-minify": "^0.4.0",
    "codecov": "^3.0.2",
    "dotenv": "^6.0.0",
    "es-check": "^2.0.3",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "rollup": "^0.64.0",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-babel-minify": "^6.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^4.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^3.0.0",
    "semantic-release": "^15.1.5",
    "travis-deploy-once": "^5.0.0"
  }
}
